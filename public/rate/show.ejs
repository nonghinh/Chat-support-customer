<% include ../partials/header %>
<% include ../partials/navbar %>
<div class="container-fluid">
	<di class="row">
		<div class="col-md-12">
			<div class="page-header">
				<h1 class="text-center">Danh sách dánh giá  chất lượng dịch vụ</h1>
			</div>
		</div>
	</di>
	<di class="row">
		<div class="col-md-12">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>#</th>
						<th>ID người dùng</th>
						<th>Đánh giá</th>
						<th>Thời gian</th>
						<th>Thao tác</th>
					</tr>
				</thead>
				<tbody>
					<% for( var i = 0; i < data.length; i++){ %>
						<tr>
							<td><%= i+1 %></td>
							<td><%= data[i].customer_id %></td>
							<td>
								<% if( data[i].level == 1){ %>
									Chưa tốt
								<% } %>
								<% if( data[i].level == 2){ %>
									Bình thường
								<% } %>
								<% if( data[i].level == 3){ %>
									Tốt
								<% } %>
								<% if( data[i].level == 4){ %>
									Rất tốt
								<% } %>
							</td>
							<td><%= data[i].datetime %></td>
							<td>
	
								<a href="/rate/delete?id=<%= data[i].id %>" title="Sửa" class="delete" onclick="return confirmDelete()"><i class="glyphicon glyphicon-trash"></i> Xóa</a>
							</td>
						</tr>
					<% }%>
				</tbody>
			</table>
		</div>
	</di>
</div>
<% include ../partials/footer %>